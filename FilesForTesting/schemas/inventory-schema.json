{
  "id": {
    "type": "uuid",
    "meta": {
      "primaryKey": true
    }
  },
  "productId": {
    "type": "uuid",
    "refTarget": "products",
    "meta": {
      "required": true,
      "label": "Product"
    }
  },
  "warehouseId": {
    "type": "string",
    "meta": {
      "required": true,
      "label": "Warehouse ID"
    }
  },
  "quantity": {
    "type": "integer",
    "meta": {
      "required": true,
      "label": "Quantity",
      "default": 0
    }
  },
  "reservedQuantity": {
    "type": "integer",
    "meta": {
      "label": "Reserved",
      "default": 0,
      "helpText": "Quantity reserved for pending orders"
    }
  },
  "reorderPoint": {
    "type": "integer",
    "meta": {
      "label": "Reorder Point",
      "helpText": "Minimum stock level before reorder"
    }
  },
  "reorderQuantity": {
    "type": "integer",
    "meta": {
      "label": "Reorder Quantity"
    }
  },
  "lastRestockedAt": {
    "type": "date",
    "meta": {
      "label": "Last Restocked"
    }
  }
}