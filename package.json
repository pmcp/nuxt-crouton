{
  "name": "nuxt-crouton-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Nuxt Crouton - Reusable CRUD layers for Nuxt 3",
  "keywords": [
    "nuxt",
    "nuxt-layer",
    "crud",
    "scaffolding",
    "translations",
    "i18n"
  ],
  "author": "FYIT",
  "license": "MIT",
  "homepage": "https://github.com/pmcp/nuxt-crouton",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmcp/nuxt-crouton.git"
  },
  "bugs": {
    "url": "https://github.com/pmcp/nuxt-crouton/issues"
  },
  "scripts": {
    "publish:all": "pnpm -r publish --access public",
    "publish:dry": "pnpm -r publish --dry-run",
    "version:patch": "pnpm -r exec npm version patch --no-git-tag-version",
    "version:minor": "pnpm -r exec npm version minor --no-git-tag-version",
    "version:major": "pnpm -r exec npm version major --no-git-tag-version",
    "clean": "rm -rf node_modules packages/*/node_modules"
  },
  "devDependencies": {
    "nuxt": "^4.0.0"
  },
  "pnpm": {
    "overrides": {
      "unimport": "4.1.1"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "es5-ext",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
