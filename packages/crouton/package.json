  "author": "FYIT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmcp/nuxt-crouton.git",
    "directory": "packages/crouton"
  },
  "bugs": {
    "url": "https://github.com/pmcp/nuxt-crouton/issues"
  },
  "homepage": "https://github.com/pmcp/nuxt-crouton#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.0"
  },
  "devDependencies": {
    "nuxt": "^4.0.0",
    "unbuild": "^3.6.1"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0"
