{
  "name": "@fyit/crouton",
  "version": "1.0.0",
  "description": "Unified Nuxt CRUD framework with opt-in features",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs"
