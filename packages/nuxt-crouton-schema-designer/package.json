{
  "name": "@friendlyinternet/nuxt-crouton-schema-designer",
  "version": "0.1.0",
  "description": "Visual schema designer for Nuxt Crouton collections with project persistence",
  "type": "module",
  "exports": {
    ".": {
      "import": "./nuxt.config.ts"
    },
    "./types": {
      "import": "./app/types/index.ts",
      "types": "./app/types/index.ts"
    },
    "./server/database/schema/schemaProjects": {
      "import": "./server/database/schema/schemaProjects.ts"
    }
  },
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "server",
    "nuxt.config.ts"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxi prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@vueuse/nuxt": "^12.0.0",
    "jiti": "^2.6.1"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.14.0",
    "@nuxt/schema": "^3.14.0",
    "@nuxthub/core": "^0.8.0",
    "nuxt": "^4.2.2",
    "typescript": "^5.7.0",
    "vue": "^3.5.25"
  },
  "peerDependencies": {
    "@friendlyinternet/nuxt-crouton": "workspace:*",
    "@friendlyinternet/nuxt-crouton-ai": "workspace:*",
    "@nuxt/ui": "^4.0.0",
    "@nuxthub/core": ">=0.8.0",
    "nuxt": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/ui": {
      "optional": true
    },
    "@friendlyinternet/nuxt-crouton-ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crouton-digital/nuxt-crouton.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-layer",
    "schema-designer",
    "crud",
    "collection",
    "visual-editor"
  ],
  "author": "Crouton Digital",
  "license": "MIT"
}
