{
  "name": "@friendlyinternet/nuxt-crouton",
  "version": "1.7.2",
  "description": "Base CRUD layer for FYIT scaffolded collections",
  "type": "module",
  "main": "./nuxt.config.ts",
  "exports": {
    ".": "./nuxt.config.ts",
    "./app/composables/*": {
      "types": "./dist/app/composables/*.d.ts",
      "import": "./dist/app/composables/*.mjs"
    },
    "./server/database/schema/*": {
      "types": "./dist/server/database/schema/*.d.ts",
      "import": "./dist/server/database/schema/*.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build",
    "sync-stub": "tsx scripts/sync-stub.ts",
    "sync-stub:fix": "tsx scripts/sync-stub.ts --fix"
  },
  "files": [
    "app",
    "server",
    "registry",
    "dist",
    "nuxt.config.ts",
    "crouton-team-auth.d.ts",
    "crouton-hooks.d.ts"
  ],
  "keywords": [
    "nuxt",
    "nuxt-layer",
    "crouton",
    "scaffolding"
  ],
  "author": "FYIT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmcp/nuxt-crouton.git",
    "directory": "packages/nuxt-crouton"
  },
  "bugs": {
    "url": "https://github.com/pmcp/nuxt-crouton/issues"
  },
  "homepage": "https://github.com/pmcp/nuxt-crouton#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "@vueuse/integrations": "^13.3.0",
    "@vueuse/nuxt": "^13.3.0",
    "sortablejs": "^1.15.6"
  },
  "peerDependencies": {
    "@friendlyinternet/nuxt-crouton-auth": "workspace:*",
    "@nuxt/ui": "^4.3.0",
    "nuxt": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@friendlyinternet/nuxt-crouton-auth": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/sortablejs": "^1.15.8",
    "tsx": "^4.19.2",
    "unbuild": "^3.6.1"
  }
}
