{
  "name": "@fyit/crouton-triage",
  "version": "0.1.0",
  "description": "Discussion-to-task triage system for Nuxt Crouton - AI analysis, multi-source adapters (Slack, Figma, Notion), domain routing",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "server",
    "schemas",
    "i18n",
    "dist",
    "nuxt.config.ts",
    "crouton.manifest.ts",
    "README.md"
  ],
  "exports": {
    ".": "./nuxt.config.ts",
    "./crouton.manifest": "./crouton.manifest.ts",
    "./server/utils/domain-routing": {
      "types": "./dist/server/utils/domain-routing.d.ts",
      "import": "./dist/server/utils/domain-routing.mjs"
    },
    "./server/utils/webhookSecurity": {
      "types": "./dist/server/utils/webhookSecurity.d.ts",
      "import": "./dist/server/utils/webhookSecurity.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build"
  },
  "keywords": [
    "nuxt",
    "nuxt-layer",
    "crouton",
    "triage",
    "discussions",
    "tasks",
    "ai",
    "slack",
    "figma",
    "notion"
  ],
  "author": "FYIT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmcp/nuxt-crouton.git",
    "directory": "packages/crouton-triage"
  },
  "bugs": {
    "url": "https://github.com/pmcp/nuxt-crouton/issues"
  },
  "homepage": "https://github.com/pmcp/nuxt-crouton#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fyit/crouton-ai": "workspace:*",
    "cheerio": "^1.1.2",
    "human-id": "^4.1.1"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0",
    "@fyit/crouton-core": "workspace:*",
    "@nuxtjs/i18n": "^9.0.0 || ^10.0.0",
    "@vueuse/core": "^11.0.0 || ^12.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@fyit/crouton-schema-designer": "workspace:*",
    "unbuild": "^3.6.1"
  }
}
