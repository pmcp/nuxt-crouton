{
  "name": "@friendlyinternet/nuxt-crouton-devtools",
  "version": "0.3.0",
  "description": "DevTools integration for Nuxt Crouton - visual inspection and management of CRUD collections",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs"
